  "bugs": {
    "url": "https://gitlab.com/kamonthep026/nasa-project/issues"
  },
  "homepage": "https://gitlab.com/kamonthep026/nasa-project#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
